{
    "name": "On start-up Extension",
    "description": "A Chrome extension that will restore the previous session!",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "/js/background.js"
    },
    "options_page": "/html/options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "/html/popup.html",
        "default_icon": {
            "16": "/images/icon-16.png",
            "32": "/images/icon-32.png",
            "48": "/images/icon-48.png",
            "128": "/images/icon-128.png"
        }
    },
    "icons": {
        "16": "/images/icon-16.png",
        "32": "/images/icon-32.png",
        "48": "/images/icon-48.png",
        "128": "/images/icon-128.png"
    }
}